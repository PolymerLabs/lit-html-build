{
  "name": "lit-html-build",
  "version": "1.0.0",
  "description": "Example rollup build for lit-html",
  "main": "index.js",
  "scripts": {
    "dev": "es-dev-server --app-index index.html --node-resolve --watch --open",
    "build": "rimraf build && rollup -c",
    "serve": "es-dev-server --app-index build/index.html --open"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "es-dev-server": "^1.47.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-minify-html-literals": "^1.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@webcomponents/template": "^1.4.2",
    "core-js": "^3.6.5",
    "lit-html": "^1.2.1",
    "regenerator-runtime": "^0.13.5"
  }
}
